
enum Gender {
    MALE
    FEMALE
}

type Artwork {
    src: String!
    height: Int
    width: Int
    type: String
}

type User{
    address: String!
    artwork: [Artwork]!
    city: String
    emailId: String
    gender: Gender
    id: Int!
    mobileNumber: String
    name: String
    zipcode: String
    account: Account
}


type Query {
    user(id: Int!): User!
}
